<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="hablar-EditBuddyWidget">
		<div class="form">
			<h2>Change buddy nickname</h2>
			<p>
				<label>Change from:</label>
				<g:Label ui:field="oldNickName">oldNickName</g:Label>
			</p>
			<p>
				<label>To:</label>
				<g:TextBox ui:field="newNickName">newNickName</g:TextBox>
			</p>
			<div class="actions">
				<g:Button ui:field="save">Change</g:Button>
				<g:Button ui:field="cancel">Cancel</g:Button>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 